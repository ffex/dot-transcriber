# Environment Variables for Dot
# Copy this file to .env and fill in your actual values

# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# OpenAI API Key (if using Whisper API or GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Groq API Key (if using Groq transcription provider)
GROQ_API_KEY=

# Optional: Override config file settings
# OLLAMA_ENDPOINT=http://localhost:11434
# LOG_LEVEL=info
